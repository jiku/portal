{
	"name": "@jiku/portal-site",
	"version": "0.1.0",
	"description": "Portal to all jiku content.",
	"private": true,
	"author": {
		"name": "B.E. Henriksen",
		"email": "e@jiku.ca",
		"url": "https://jiku.ca"
	},
	"repository": {
		"type": "git",
		"url": "http://github.com/jiku/portal-site.git"
	},
	"bugs": {
		"url": "http://github.com/jiku/portal-site/issues"
	},
	"scripts": {
		"start": "cd src && npm run start",
		"check": "npm-check && ncu",
		"edit": "code-insiders .",
		"readme": "markdown-cli README.md",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"engines": {
		"node": ">=8"
	},
	"dependencies": {
		"babel-runtime": "^6.9.2",
		"react": "^15.2.0",
		"react-dom": "^15.2.0"
	},
	"devDependencies": {
		"babel-cli": "^6.10.1",
		"babel-core": "^6.10.4",
		"babel-plugin-espower": "^2.3.1",
		"babel-plugin-transform-runtime": "^6.9.0",
		"babel-preset-es2015": "^6.9.0",
		"babel-preset-stage-0": "^6.5.0",
		"babel-preset-react": "^6.11.1",
		"babelify": "^7.3.0",
		"@kadira/storybook": "^2.5.2"
	},
	"babel": {
		"presets": [
			"es2015",
			"stage-0"
		],
		"plugins": [
			[
				"transform-runtime",
				{
					"polyfill": false,
					"regenerator": true
				}
			],
			"espower"
		]
	},
	"keywords": [
		"jiku",
		"Portal",
		"creative",
		"outlet"
	],
	"license": "MIT"
}
